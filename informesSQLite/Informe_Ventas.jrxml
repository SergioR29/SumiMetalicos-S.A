	<queryString language="SQL">
		<![CDATA[SELECT C.Nombre, COUNT(*) as n_articulos, strftime("%m", p.Fecha_Pedido) as mes_n,
CASE strftime("%m", p.Fecha_Pedido) 
           WHEN '01' THEN 'Enero'
           WHEN '02' THEN 'Febrero'
           WHEN '03' THEN 'Marzo'
           WHEN '04' THEN 'Abril'
           WHEN '05' THEN 'Mayo'
           WHEN '06' THEN 'Junio'
           WHEN '07' THEN 'Julio'
           WHEN '08' THEN 'Agosto'
           WHEN '09' THEN 'Septiembre'
           WHEN '10' THEN 'Octubre'
           WHEN '11' THEN 'Noviembre'
           WHEN '12' THEN 'Diciembre'
       END AS mes
FROM pedidos P, clientes C, detalle_pedidos DP, articulos A 
WHERE P.Id_cliente = C.Id_cliente AND DP.ID_Pedido = P.ID_Pedido AND DP.Id_articulo = A.Id_articulo
GROUP BY C.Id_cliente, mes
ORDER BY mes_n]]>
	</queryString>
