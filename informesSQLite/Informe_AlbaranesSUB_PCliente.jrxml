	<parameter name="ID__Cliente" class="java.lang.Long"/>
	<queryString>
		<![CDATA[SELECT C."Nombre",
	C."Direccion",
	C."Ciudad",
	T."Telefono",
	E."Email",
	P."ID_Pedido",
	P."Fecha_Pedido", 
	DP."ID_Venta",
	A."Descripcion", 
	DP."Precio", 
	DP."Unidades", 
	(DP."Precio" * DP."Unidades") AS C_Venta
FROM clientes C,
	telefonos T,
	emails E,
	pedidos P, 
	detalle_pedidos DP, 
	articulos A
WHERE 
	 C."ID_Cliente" = $P{ID__Cliente}
	 AND T."Id_Cliente" = $P{ID__Cliente}
	 AND E."Id_cliente" = $P{ID__Cliente}
	 AND P."Id_Cliente" = $P{ID__Cliente}
	 AND DP."ID_Pedido" = P."ID_Pedido" 
	 AND DP."ID_Articulo" = A."ID_Articulo"
GROUP BY
		P."ID_Pedido", A."Descripcion"]]>
	</queryString>
